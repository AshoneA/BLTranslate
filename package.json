{
  "name": "BLTranslate",
  "displayName": "BLTranslate",
  "description": "",
  "version": "0.0.6",
  "publisher": "zefengbao",
  "engines": {
    "vscode": "^1.37.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:extension.BLTranslate",
    "onCommand:extension.BLi18n"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "extension.BLTranslate",
        "title": "BLTranslate"
      },
      {
        "command": "extension.BLi18n",
        "title": "BLi18n"
      }
    ],
    "menus": {
      "editor/context": [
        {
          "command": "extension.BLTranslate"
        },
        {
          "command": "extension.BLi18n"
        }
      ]
    },
    "keybindings": [
      {
        "command": "extension.BLTranslate",
        "key": "ctrl+i",
        "mac": "cmd+i",
        "when": "editorTextFocus"
      },
      {
        "command": "extension.BLi18n",
        "key": "cmd+shift+i",
        "mac": "cmd+shift+i",
        "when": "editorTextFocus"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/glob": "^7.1.1",
    "@types/mocha": "^5.2.6",
    "@types/node": "^10.12.21",
    "@types/vscode": "^1.37.0",
    "glob": "^7.1.4",
    "mocha": "^6.1.4",
    "typescript": "^3.3.1",
    "tslint": "^5.12.1",
    "vscode-test": "^1.0.2"
  },
  "dependencies": {
    "@vitalets/google-translate-api": "^3.0.0"
  }
}
